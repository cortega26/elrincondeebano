    "cypress": "^15.9.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "jsdom": "^28.0.0",
    "lighthouse": "13.0.1",
    "prettier": "^3.8.1",
    "serve": "^14.2.5",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "undici": "^7.20.0",
    "vitest": "^4.0.16"
  },
